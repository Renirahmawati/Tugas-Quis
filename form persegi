import newpackage.perhitungan.persegi;
public class new_persegi extends javax.swing.JPanel {
  public new_persegi() {
        initComponents();
    }
private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
    persegi a = new persegi();
    int sisi = Integer.parseInt(jTextField1.getText());
    int hasil = a.luaspersegi(sisi);
    
    jLabel3.setText("hasil = "+hasil);
    }                                                      
